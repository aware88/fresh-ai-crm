# AI Memory System Test Environment
# Copy this file to .env and update the values

# API Base URL (default: http://localhost:3000)
API_BASE_URL=http://localhost:3000

# Authentication token (required)
# Get this from your browser's local storage after logging in
AUTH_TOKEN=your_auth_token_here

# OpenAI API Key (required for embedding generation)
OPENAI_API_KEY=your_openai_api_key_here

# Test organization ID (optional, will use the one from auth token if not provided)
TEST_ORGANIZATION_ID=

# Debug mode (optional, set to true for verbose logging)
DEBUG_AI_MEMORY=false
