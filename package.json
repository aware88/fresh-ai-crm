    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:setup": "node scripts/setup-database.js",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "db:migrate:create": "node scripts/create-migration.js $npm_config_name",
    "db:migrate:status": "psql $SUPABASE_DB_URL -c \"SELECT name, applied_at FROM public.migrations ORDER BY applied_at DESC;\"",
    "db:seed": "chmod +x ./scripts/seed-database.sh && ./scripts/seed-database.sh",
    "db:seed:contacts": "psql $SUPABASE_DB_URL -f scripts/seed-contacts.sql",
    "db:seed:interactions": "psql $SUPABASE_DB_URL -f scripts/seed-interactions.sql",
    "db:seed:files": "psql $SUPABASE_DB_URL -f scripts/seed-files.sql",
    "test:email": "tsx scripts/test-email.ts",
    "test:email-analysis": "tsx scripts/test-email-analysis.ts",
    "alert:process": "node scripts/run-alert-processor.js",
    "alert:process:watch": "nodemon --watch src/workers --ext ts,js --exec 'npm run alert:process'",
    "setup": "node scripts/setup.js",
    "check-env": "node scripts/check-env.js",
    "check:env": "node scripts/check-env.js",
    "doctor": "node scripts/check-env.js",
    "cleanup:all": "node scripts/cleanup-all-data.js",
    "cleanup:data": "node scripts/cleanup-all-data.js",
    "test:ai-agents": "node scripts/test-ai-agents.js",
    "test:ai-agents:quick": "node scripts/test-ai-agents.js --quick",
    "test:production-ready": "node scripts/production-readiness-check.js",
    "pre-deploy": "npm run test:production-ready && npm run test:ai-agents:quick",
    "validate-system": "node scripts/production-readiness-check.js --verbose && node scripts/test-ai-agents.js --api"
