    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^8.6.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.0.0",
    "dotenv": "^16.6.1",
