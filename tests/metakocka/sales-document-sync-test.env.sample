# Sample environment variables for sales document sync test
# Copy this file to .env and update the values

# Authentication token for API requests
AUTH_TOKEN=your_auth_token_here

# User ID for test mode authentication
USER_ID=00000000-0000-0000-0000-000000000000

# API base URL (default: http://localhost:3001)
API_BASE_URL=http://localhost:3001

# Sales document ID from the CRM database
DOCUMENT_ID=your_document_id_here

# Metakocka ID for reverse sync test (optional)
METAKOCKA_ID=your_metakocka_id_here
