# Authentication token for API requests
AUTH_TOKEN=your_auth_token_here

# User ID for test mode authentication
USER_ID=your_user_id_here

# Test mode - set to true to create and clean up test data automatically
# If false, you must provide DOCUMENT_ID
TEST_MODE=true

# Document ID to use for testing when TEST_MODE=false
# Only required if TEST_MODE is not enabled
DOCUMENT_ID=

# Base URL for API requests
API_BASE_URL=http://localhost:3001/api
